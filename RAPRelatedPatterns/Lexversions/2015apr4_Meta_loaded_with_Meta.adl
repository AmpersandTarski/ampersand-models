{-311-}SPEC DerivedRelation ISA Expression
{-312-}
{-313-}RELATION has_left[DerivedRelation*Expression] [UNI,TOT]
{-314-}RELATION uses[DerivedRelation*Operator] [UNI,TOT]
{-315-}RELATION has_right[DerivedRelation*Expression] [UNI]
{-316-}-- unary operators have no righthand side, i.e. postfix notation is used in unary operators
{-317-}
{-318-}RULE "04_DerivedRelation_is_unique_by_its_left_and_operator_and_either_its_right_or_the_operator_is_not_binary" : 
{-319-}has_left;has_left~ /\ uses;uses~ /\ ((has_right;has_right~)\/(uses;(I[Operator]/\-('compose'\/'unite'\/'intersect'\/'minus'\/'sunim'));uses~)) |- I[DerivedRelation] 
{-320-}
{-321-}-- RULE "04s_DerivedRelation_is_ultimately_determined_by_other_than_DerivedRelations" : ((has_left.or.has_right).kleene-star).range |- I[BaseRelation]
{-322-}
{-323-}-- RULE "04c_Avoid_redundancy_of_DerivedRelations_due_to_Commutative_SetOperators" : RULE as (uses;CommutativeSetOperator).domain.^.(has_left;has_right.inverse) MUST BE IRREFLEXIVE
{-324-}
{-325-}RULE "05d_DerivedRelation_has_domain_determined_via_operator" : I[DerivedRelation]; has_domain |- 
{-326-}((I[DerivedRelation]/\(uses;(I[Operator]/\-('range'\/'check_inj'\/'check_sur'\/'inverse'));uses~)); has_left;has_domain \/
{-327-} (I[DerivedRelation]/\(uses;('range'\/'check_inj'\/'check_sur'\/'inverse');uses~)); has_left;has_range ) ; (I[BaseConcept]\/isa~)
{-328-}-- IF operator is binary (i.e. compose, or one of its clones) THEN DerivedRelation has domain = domain of lefthand relation
{-329-}-- IF unary operator is range or check_inj or check_sur or inverse THEN DerivedRelation has domain = range of lefthand relation
{-330-}-- IF operator is unary EXCEPT range or check_inj or check_sur or inverse THEN DerivedRelation has domain = domain of lefthand relation
{-331-}-- the final clause (I[BaseConcept]\/isa~) is needed to allow for ISA relations: the DerivedRelation domain may be defined on a specialization of original Expression's domain (never a generalization)
{-332-}
{-333-}-- rewrite of old-formula using improved notations/operators
{-334-}-- I[DerivedRelation]/\(uses;(I[Operator]/\-('range'\/'check_inj'\/'check_sur'\/'inverse'));uses.inverse) comes down to: (uses;Operator-minus-Domain-reversing-SetOperator).domain
{-335-}-- I[DerivedRelation]/\(uses;('range'\/'check_inj'\/'check_sur'\/'inverse');uses.inverse) comes down to: (uses;Domain-reversing-SetOperator).domain
{-336-}
{-337-}RULE "05r_DerivedRelation_has_range_determined_via_operator" : I[DerivedRelation]; has_range |- 
{-338-}((I[DerivedRelation]/\(uses;('compose');uses~)); has_right;has_range \/ 
{-339-} (I[DerivedRelation]/\(uses;(I[Operator]/\-('compose'\/'domain'\/'check_uni'\/'check_tot'\/'inverse'));uses~)); has_left;has_range \/
{-340-} (I[DerivedRelation]/\(uses;('domain'\/'check_uni'\/'check_tot'\/'inverse');uses~)); has_left;has_domain ) ; (I[BaseConcept]\/isa~)
{-341-}-- IF operator is binary (i.e. compose, or one of its clones) THEN DerivedRelation has range = range of righthand relation
{-342-}-- IF unary operator is domain or check_uni or check_tot or inverse THEN DerivedRelation has range = domain of lefthand relation
{-343-}-- IF operator is unary EXCEPT domain or check_uni or check_tot or inverse THEN DerivedRelation has range = range of lefthand relation
{-344-}
{-345-}-- IF operator is binary AND Setlike (i.e. unite or intersect or minus or sunim, but not compose) THEN domain (and range) of left = domain (and range) of right, but allow for generalizations
{-346-}-- rewrite of old-formula lefthand side, using improved notations/operators:
{-347-}-- I[DerivedRelation]/\(uses;('unite'\/'intersect'\/'minus'\/'sunim');uses.inverse) comes down to: (uses;BinarySetOperator).domain
{-348-}
{-349-}RULE "06b_BinarySetOperator_requires_relations_have_similar_domains_and_ranges" : 
{-350-}I[DerivedRelation]/\ (uses;('unite'\/'intersect'\/'minus'\/'sunim');uses~) |- (has_left;has_domain);(isa\/I[BaseConcept]\/isa~);(has_right;has_domain)~ /\ (has_left;has_range);(isa\/I[BaseConcept]\/isa~);(has_right;has_range)~
{-351-}
{-352-}-- IF operator is binary AND Relational (i.e. compose (or any of its clones)) THEN range of left = domain of right, but allow for generalizations
{-353-}-- rewrite of old-formula using improved notations/operators:
{-354-}-- I[DerivedRelation]/\(uses;('compose');uses.inverse) comes down to: (uses;RelationalOperator).domain
{-355-}
{-356-}RULE "06r_RelationalOperator_requires_range_of_left_expression_similar_to_domain_of_right_expression" : 
{-357-}I[DerivedRelation]/\ (uses;'compose';uses~) |- (has_left;has_range);(isa\/I[BaseConcept]\/isa~);(has_right;has_domain)~

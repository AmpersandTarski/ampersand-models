<?xml version="1.0" encoding="ASCII"?>
<dw:Component xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:dw="http://com.siemens.ct.datawires.model" name="XMPPClient">
  <artifactId groupId="com.siemens.ct.xmpp.generic-client" artifactId="XMPPClient" version="0.1.0.SNAPSHOT"/>
  <interface>
    <input name="host" type="{http://www.siemens.com/DataWires/BasicTypes}String" maxWires="1" const="true" default="localhost"/>
    <input name="port" type="{http://www.siemens.com/DataWires/BasicTypes}Integer" maxWires="1" const="true" default="5222"/>
    <input name="service" type="{http://www.siemens.com/DataWires/BasicTypes}String" maxWires="1" const="true"/>
    <input name="resource" type="{http://www.siemens.com/DataWires/BasicTypes}String" maxWires="1" const="true" default="SiemensCT"/>
    <input name="compression" type="{http://www.siemens.com/DataWires/BasicTypes}Boolean" maxWires="1" const="true" default="true"/>
    <input name="debug" type="{http://www.siemens.com/DataWires/BasicTypes}Boolean" maxWires="1" const="true" default="false"/>
    <input name="securitymode" type="{http://www.siemens.com/DataWires/BasicTypes}String" maxWires="1" const="true" default="ifpossible"/>
    <input name="trustallcerts" type="{http://www.siemens.com/DataWires/BasicTypes}Boolean" maxWires="1" const="true" default="false"/>
    <input name="trustcert" type="{http://www.siemens.com/DataWires/BasicTypes}String" maxWires="1" const="true"/>
    <input name="allowallhostnames" type="{http://www.siemens.com/DataWires/BasicTypes}Boolean" maxWires="1" const="true" default="false"/>
    <input name="createaccount" type="{http://www.siemens.com/DataWires/BasicTypes}Boolean" maxWires="1" const="true" default="false"/>
    <input name="retryinterval" type="{http://www.siemens.com/DataWires/BasicTypes}Integer" maxWires="1" const="true" default="30000"/>
    <input name="reconnectafterclose" type="{http://www.siemens.com/DataWires/BasicTypes}Boolean" maxWires="1" const="true" default="true"/>
    <input name="user" type="{http://www.siemens.com/DataWires/BasicTypes}String" maxWires="1" const="true"/>
    <input name="password" type="{http://www.siemens.com/DataWires/BasicTypes}String" maxWires="1" const="true"/>
    <output name="connection" type="{http://www.siemens.com/DataWires/BasicTypes}ObjRef"/>
    <output name="connected" type="{http://www.siemens.com/DataWires/BasicTypes}Boolean"/>
  </interface>
</dw:Component>

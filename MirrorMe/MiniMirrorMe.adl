   [ scope : I CRUD
   ]

INTERFACE "Overzicht": I[Scope] CRUD COLS
   [ "ID" : argument CRUD 
   , "claim": argument BOX
	[" " : claim 
	]
   ]

INTERFACE " ": I[Argument] COLS
   [ "  " : claim cRud COLS
		["claim" : I
		, "validation": ttext_validation
		]
	, "onderbouwing": supports~;I[Justification] COLS
		 [ "identifier" : I 
		, "onderbouwend feit"  : datum 
   		, "rechtsregel"	: warrant
   		, "rechtsgrond"	: backing
   		, "tegenargument" : rebuttal  
		, "validation" : justification_validation
   		]
        , "uitzondering op": rebuttal~ COLS 
		["datum" : datum
		, "warrant" : warrant
		]
	, "onderbouwd door": sub~ cRud
   ]

INTERFACE "Argumentatie": I[Justification] cRud COLS
   [ "claim" : supports;claim cRud
   , " "  : datum cRud  COLS
		["datum" : I
		, "validation" : ttext_validation 	
		]
   , "  "	: warrant cRud COLS
		["warrant": I
		, "validation" : ttext_validation
		]
   , "rechtsgrond"	: backing cRud 
   , "cumulation": justification_cumulation
   ] 

INTERFACE "Validation": I[Validation] COLS
   [ "statement": ttext_validation~ cRud
   , "    " : justification_validation~ cRud COLS
	[ "datum" : datum cRud
        , "warrant" : warrant cRud
	]
   , "     " : cumulation_validation~ cRud
   , "waar of onwaar" : position cRUD
   , "partij" : party
   , "onderbouwing" : explanation
   ]

INTERFACE "Sub argument": V[SESSION*Argument] COLS
  [ "  " : I 
  , "add claim" : I COLS
	[ "claim": claim
	]
  ]

INTERFACE "Cumulatie argumenten": I[Cumulation] COLS
   [" " : I COLS
	["justification" : justification_cumulation~ COLS
		["datum" : datum
		, "warrant" : warrant
		]
	]
   , "validation" : cumulation_validation  
   ]



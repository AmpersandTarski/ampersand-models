    CONTEXT MiniMirrorMe

--INCLUDE "Reasoning.adl"
INCLUDE "../TText/Scope_SIAM_Extensions.adl"  -- SIAM Extensions for Scopes (e.g. Scope Ownership)
--INCLUDE "../TText/TParty_SIAM_Extensions.adl" -- SIAM Extensions for TTexts (e.g. TText Ownership)

	RELATION argument[Scope*Argument] [INJ] -- Complex van claim(s), datum(s), warrant(s), etc.
	RELATION claim[Argument*TText] [UNI, INJ]-- Conclusion that needs to be proved
	RELATION supports[Justification*Argument] 
	RELATION datum [Justification*TText]
	RELATION warrant [Justification*TText] [UNI]
    	RELATION backing[Justification*TText] -- Supports and authorizes the warrant
	RELATION rebuttal [Justification*Argument]
    	RELATION sub [Argument*Argument]

	RELATION justification_cumulation [Justification*Cumulation]

	RELATION ttext_validation [TText*Validation]
	RELATION justification_validation [Justification*Validation] 
	RELATION cumulation_validation [Cumulation*Validation]
	RELATION position [Validation*Position] [UNI]
	RELATION party [Validation*Actor] [UNI]
	RELATION explanation [Validation*Argument]
	{- RELATION validation_sub [Validation*Argument] -}

PATTERN Twovalued
RULE twovalued : I[Position] = "valid" \/ "invalid"
VIOLATION (SRC I, TXT " is not a Position. Please pick 'valid' or 'invalid'.")
ENDPATTERN


REPRESENT String TYPE BIGALPHANUMERIC
RELATION text[TText*String] [UNI]
VIEW TText : TText(text)

CLASSIFY Argument ISA TText

{- Intermezzo: Wat nu volgt is bedoeld om de relatie sub[Argument*Argument] automatisch uit te rekenen. -}

ROLE ExecEngine MAINTAINS "inserting pairs into sub", "deleting pairs from sub"

RULE "inserting pairs into sub":
   claim;datum~;supports |- sub

MEANING "If the claim of argument a is a datum of argument c, then a is subordinate to c."
VIOLATION (TXT "{EX} InsPair;sub;Argument;", SRC I, TXT ";Argument;", TGT I)

RULE "deleting pairs from sub":
   sub |- claim;datum~;supports
MEANING "If argument a is subordinate to argument c, then the claim of a is a datum of c."
VIOLATION (TXT "{EX} DelPair;sub;Argument;", SRC I, TXT ";Argument;", TGT I)

{- Het uitrekenen gebeurt in twee stappen. Eén stap leidt tot het invoegen van een paar in sub, als veranderingen in claim, datum en rebuttal daar aanleiding toe geven.
Het verwijderen van elementen uit sub gebeurt in de tweede regel.
Einde Intermezzo -}

{-ROLE ExecEngine MAINTAINS "inserting pairs into validation_sub", "deleting pairs from validation_sub"

RULE "inserting pairs into validation_sub":
   ttext_validation~;claim~\/justification_validation~;supports |- validation_sub
VIOLATION (TXT "{EX} InsPair;validation_sub;Validation;", SRC I, TXT ";Argument;", TGT I)

RULE "deleting pairs from validation_sub":
   validation_sub |- ttext_validation~;claim~\/justification_validation~;supports
VIOLATION (TXT "{EX} DelPair;validation_claim;Validation;", SRC I, TXT ";Argument;", TGT I)-}


